{
    "name": "tranquillity/tranquillity-api",
    "description": "Tranquillity REST API",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tranquillity\\": "src/"
        },
        "files": [
            "src/_scripts/environment.php"
        ]
    },
    "require": {
        "php-di/php-di": "^6.2",
        "slim/slim": "^4.0",
        "monolog/monolog": "^1.23",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "symfony/dotenv": "^5.1",
        "symfony/finder": "^4.0",
        "doctrine/orm": "^2.6",
        "doctrine/migrations": "^3.0",
        "bshaffer/oauth2-server-php": "^1.10",
        "justinrainbow/json-schema": "2.0.0",
        "nesbot/carbon": "^2.2",
        "ramsey/uuid": "4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "robmorgan/phinx": "^0.9.2",
        "seld/jsonlint": "1.4.0",
        "vlucas/valitron": "^1.4",
        "woohoolabs/yin": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
